{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "freemius-blocks/buy-button",
  "version": "0.6.0",
  "title": "Freemius Buy Button",
  "category": "text",
  "icon": "flag",
  "description": "Add a Freemius buy button",
  "supports": {
    "html": false
  },
  "attributes": {
    "plugin_name": {
      "type": "string"
    },
    "plugin_id": {
      "type": "string"
    },
    "plan_id": {
      "type": "string"
    },
    "quantity": {
      "type": "string",
      "default": 1
    },
    "billing_cycle": {
      "type": "string"
    },
    "buttonLabel": {
      "type": "string"
    }
  },
  "textdomain": "freemius-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.scss",
  "style": "file:./style.scss"
}